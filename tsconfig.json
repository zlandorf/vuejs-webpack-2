{
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*.ts"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "es5",
      "es2015.promise"
    ],
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5"
  }
}
